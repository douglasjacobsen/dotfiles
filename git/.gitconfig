[user]
	name = Douglas Jacobsen
	email = jacobsen.douglas@gmail.com
[core]
	editor = vim
[giggle]
	main-window-maximized = false
	main-window-geometry = 944x754+213+24
	main-window-view = HistoryView
	file-view-vpane-position = 392
	history-view-vpane-position = 456
[web]
	browser = google-chrome
[alias]
	logg = log --graph --oneline --abbrev-commit --decorate
	export = !sh -c 'git archive ${1} | tar xv -C ${2}' -
	st = status
    up = fetch --all -p
[color]
	ui = true
[merge]
	tool = vimdiff
	summary = true
[github]
	user = douglasjacobsen
[push]
	default = current
[http]
	proxy = 
